<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mvvm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mvvm"
        android:usesCleartextTraffic="true">
        <activity android:name=".search.SearchActivity"></activity>
        <activity android:name=".orderDetails.OrderDetialsActivity" />
        <activity android:name=".order.OrderActivity" />
        <activity android:name=".buy.BuyActivity" />
        <activity android:name=".checkout.CheckoutActivity" />
        <activity android:name=".listcart.ListCartActivity" />
        <activity android:name=".categorylist.CategortListActivity" />
        <activity
            android:name=".auth.AuthenticationActivity"
            android:launchMode="singleInstance" />
        <activity android:name=".details.SingleProductActivity" />
        <activity android:name=".listproduct.ShowListActivity" />
        <activity android:name=".home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>